{
    "name": "shentx/flarum-daily-check-in",
    "description": "一个 Flarum 拓展. 在 Ziiven/flarum-daily-check-in 签到基础上增加了随机积分功能.",
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.8"
    },
    "support": {
        "issues": "https://github.com/AppLinko/flarum-daily-check-in/issues",
        "source": "https://github.com/AppLinko/flarum-daily-check-in"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AppLinko\\checkin\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Shentx"
        }
    ],
    "extra": {
        "flarum-extension": {
            "title": "Shentx Daily Check In",
            "icon": {
                "name": "fas fa-calendar-check",
                "backgroundColor": "#238c59",
                "color": "#fff"
            }
        }
    }
}
